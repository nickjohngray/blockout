{
  "name": "blockout",
  "version": "1.0.25",
  "description": "Building Blocks for React that think outside the box",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "upload": "npm publish",
    "test": "jest --config jestconfig.json",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": "git+https://github.com/nickjohngray/blockout.git",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@nickjohngray"
  },
  "files": [
    "lib/**/*"
  ],
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "prepublishOnly": "npm run build && cp -r ./build/* . && rm -rf ./build",
  "postpublish": "git clean -fd",
  "keywords": [
    "typescript",
    "github packages",
    "npm packages",
    "React components"
  ],
  "author": "Nick John Gray",
  "license": "Apache-2.0",
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.8",
    "@types/react": "^16.9.44",
    "@types/react-dom": "^16.9.8",
    "eslint": "^7.5.0",
    "jest": "^26.2.2",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.4",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.4.5"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
